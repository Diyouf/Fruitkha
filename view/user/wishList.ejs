<%- include("../layout/homeHeader.ejs") %>


    <!--PreLoader Ends-->

    <!-- header -->
    <div class="top-header-area" id="sticker">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 col-sm-12 text-center">
                    <div class="main-menu-wrap">
                        <!-- logo -->
                        <div class="site-logo">
                            <a href="index.html">
                                <img src="assets/img/logo.png" alt="">
                            </a>
                        </div>
                        <!-- logo -->

                        <!-- menu start -->
                        <nav class="main-menu">
                            <ul>
                                <li class="current-list-item"><a href="/">Home</a>

                                </li>
                                <li><a href="/about">About</a></li>
                                <li><a href="/contact">Contact</a></li>
                                <li><a href="/shop">Shop</a></li>
                                <li><a href="/cart">Cart</a></li>
                                <li><a href="/wishList">WishList</a></li>
                                <li>
                                    <div class="header-icons">
                                        <% if(typeof userData !="undefined" ){ %>
                                            <a class="dropks" href="">

                                                <%= userData.name %>

                                            </a>
                                            <ul class="sub-menu">
                                                <li><a href="/userProfile">Profile</a></li>
                                                <li><a href="/orderList">Orders</a></li>
                                                <li><a href="/userAddress">Address</a></li>
                                                <li><a href="/logout"><button class="btn"
                                                            style="background-color: orange;">Logout</button></a></li>

                                            </ul>
                                            <% }else{ %>
                                                <a href="/login">login</a>
                                                <% } %>


                                    </div>
                                </li>
                            </ul>
                        </nav>
                        <a class="mobile-show search-bar-icon" href="#"><i class="fas fa-search"></i></a>
                        <div class="mobile-menu"></div>
                        <!-- menu end -->
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!-- breadcrumb-section -->
    <div class="breadcrumb-section breadcrumb-bg">
        <div class="container">
            <div class="row">
                <div class="col-lg-8 offset-lg-2 text-center">
                    <div class="breadcrumb-text">
                        <h1>Wishlist</h1>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- end breadcrumb section -->

    <!-- cart -->
    <div class="cart-section mt-150 mb-150">
        <% if(typeof userData !=='undefined' ){ %>
            <div class="container">
                <div class="row">
                    <div class="col-lg-12 col-md-12">
                        <div class="cart-table-wrap">
                            <% if(typeof wishData == 'undefined' || wishData.length ===0) {%>

                                
                                <h2>Wish List is Empty</h2>
                                <div class="cart-buttons ">
                                    <a href="/shop" class="boxed-btn">Continue Shopping</a>
                                </div>
                                <% }else{ %>
                                    <table class="cart-table">
                                        <thead class="cart-table-head">
                                            <tr class="table-head-row">
                                                <th class="product-remove">Remove</th>
                                                <th class="product-name"><b>product Image </b></th>
                                                <th class="product-name"><b>product Name </b></th>
                                                <th class="product-price"><b>Sale Price</b></th>
                                                <th class="product-quantity"><b>Move to cart</b></th>
    
                                            </tr>
                                        </thead>
                                        <% wishData.forEach((product)=>{ %>
                                            <tbody>
                                                <td class="product-remove"><a
                                                        href="/removeWishpro/?id=<%= product.productId._id %>"><i
                                                            class="far fa-window-close"></i></a>
                                                <td class="product-image">
                                                    <img src="/ProductImage/<%= product.productId.image[0] %>" alt="">
                                                </td>
                                                <td class="text-center">
                                                    <%= product.productId.productName%>
                                                </td>
                                                <td class="text-center">
                                                    â‚¹ <%= product.productId.salePrice%>
                                                </td>
                                                <td><a href="/moveToCart/?id=<%= product.productId._id %>"
                                                        class="btn btn-dark"><svg xmlns="http://www.w3.org/2000/svg"
                                                            width="16" height="16" fill="white" class="bi bi-cart-plus"
                                                            viewBox="0 0 16 16">
                                                            <path
                                                                d="M9 5.5a.5.5 0 0 0-1 0V7H6.5a.5.5 0 0 0 0 1H8v1.5a.5.5 0 0 0 1 0V8h1.5a.5.5 0 0 0 0-1H9V5.5z" />
                                                            <path
                                                                d="M.5 1a.5.5 0 0 0 0 1h1.11l.401 1.607 1.498 7.985A.5.5 0 0 0 4 12h1a2 2 0 1 0 0 4 2 2 0 0 0 0-4h7a2 2 0 1 0 0 4 2 2 0 0 0 0-4h1a.5.5 0 0 0 .491-.408l1.5-8A.5.5 0 0 0 14.5 3H2.89l-.405-1.621A.5.5 0 0 0 2 1H.5zm3.915 10L3.102 4h10.796l-1.313 7h-8.17zM6 14a1 1 0 1 1-2 0 1 1 0 0 1 2 0zm7 0a1 1 0 1 1-2 0 1 1 0 0 1 2 0z" />
                                                        </svg></a></td>
                                            </tbody>
                                            <% }) %>
                                    </table>
                                    <% } %>
                        </div>
                    </div>

                </div>

            </div>
            <% }else{ %>

                <div class="container">
                    <div class="row">
                        <div class="col-lg-8 col-md-12">
                            <div class="cart-table-wrap">
                                <h2>Login Your Acount</h2>
                                <div class="cart-buttons ">
                                    <a href="/login" class="boxed-btn">login Account</a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <% } %>
    </div>
    <!-- end cart -->



