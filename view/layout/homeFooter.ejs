<script>
    function changeqnty(userCart, productId, id, salePrice, quantity) {
        $.ajax({
            type: "POST",
            url: '/change-product-quantity',
            data: {
                userData: userCart,
                productId: productId,
                _id: id,
                quantity: quantity,


            },
            success: (response) => {

                if (response.success) {
                    let value = document.getElementById(productId).value
                    document.getElementById(productId).value = Number(value) + 1
                    document.getElementById(id).innerHTML = response.total + Number(salePrice)
                    let st = document.getElementById("totalSP").innerHTML;
                    document.getElementById("totalSP").innerHTML = Number(st) + Number(salePrice)
                    let tP = document.getElementById("totalTP").innerHTML;
                    document.getElementById("totalTP").innerHTML = Number(tP) + Number(salePrice)

                } else if (response.success == false) {
                    let value = document.getElementById(productId).value
                    document.getElementById(productId).value = Number(value) - 1
                    document.getElementById(id).innerHTML = response.total - Number(salePrice)
                    let st = document.getElementById("totalSP").innerHTML;
                    document.getElementById("totalSP").innerHTML = Number(st) - Number(salePrice)
                    let tP = document.getElementById("totalTP").innerHTML;
                    document.getElementById("totalTP").innerHTML = Number(tP) - Number(salePrice)
                } else {
                    Swal.fire(
                        'Product is Deleted !',
                        'Product is Deleted from the Cart ',
                        'warning'
                    ).then((result) => {
                        location.reload();
                    })
                }


            },
        })
    }
</script>
